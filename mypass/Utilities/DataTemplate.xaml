<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"                   
                    xmlns:vm="clr-namespace:mypass.ViewModel"
                    xmlns:view="clr-namespace:mypass.View"
                    >

    <DataTemplate DataType="{x:Type vm:AccountsVM}">
        <view:Accounts />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AllPassesVM}">
        <view:AllPasses/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CardsVM}">
        <view:Cards/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MainMenuVM}">
        <view:MainMenu/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PassCheckVM}">
        <view:PassCheck/>
    </DataTemplate>

    <!--<DataTemplate DataType="{x:Type vm:PassGenVM}">
        <view:PassGen/>
    </DataTemplate>-->

    <DataTemplate DataType="{x:Type vm:EmailsVM}">
        <view:Emails />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NofiticationsVM}">
        <view:Nofitications/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VhodVM}">
        <view:Vhod/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegistrationPageVM}">
        <view:RegistrationPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MainAuthWindowVM}">
        <view:MainAuthWindow/>
    </DataTemplate>

</ResourceDictionary>