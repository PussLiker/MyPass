<Window x:Class="mypass.View.PassGenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mypass.View"
        
        mc:Ignorable="d"
        Title="PassGenWindow" Height="300" Width="400" WindowStartupLocation="CenterScreen"
        WindowStyle="None" Background="Transparent" AllowsTransparency="True" MaxHeight="300" MaxWidth="400">
    <Grid MouseDown="Grid_MouseDown" >
        <ContentControl>
            <local:PassGen Loaded="PassGen_Loaded" />
        </ContentControl>
    </Grid>
</Window>
